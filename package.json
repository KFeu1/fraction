{
  "name": "precise-fractions",
  "private": false,
  "author": "Konstantin Feuerborn",
  "version": "1.1.8",
  "type": "module",
  "license": "MIT",
  "declaration": true,
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.ts",
